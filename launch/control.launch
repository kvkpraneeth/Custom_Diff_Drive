<?xml version="1.0"?>

<launch>

  <rosparam file="$(find turtlebot)/config/control.yaml" command="load"/>

  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="wheel_left_controller wheel_right_controller joint_state_controller"/>

  <rosparam file="$(find turtlebot)/config/diff_drive.yaml" command="load"/>

  <node name="controller" pkg="turtlebot" type="controller" output="screen"/>

</launch>
